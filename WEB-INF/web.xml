<?xml version="1.0" encoding="UTF-8"?>
<web-app>

    
    <servlet>
        <servlet-name>GitInputServlet</servlet-name>
        <servlet-class>GitInputServlet</servlet-class>
    </servlet>

    <!-- サーブレットのURLマッピング -->
    <servlet-mapping>
        <servlet-name>GitInputServlet</servlet-name>
        <url-pattern>/GitInputServlet</url-pattern>

    </servlet-mapping>
        <servlet>
        <servlet-name>gitinput</servlet-name>
        <jsp-file>/gitinput.jsp</jsp-file>
    </servlet>

    <!-- サーブレットのURLマッピング -->
    <servlet-mapping>
        <servlet-name>gitinput</servlet-name>
        <url-pattern>/gitinput</url-pattern>
    </servlet-mapping>

    
</web-app>